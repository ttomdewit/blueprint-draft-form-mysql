  "require": {
    "doctrine/dbal": "^2.10",
    "illuminate/support": "^7.6"
  },
  "require-dev": {
    "phpunit/phpunit": "  ^9.1",
    "orchestra/testbench": "^5.1",
    "vimeo/psalm": "^3.11",
    "phpstan/phpstan": "^0.12.18",
    "friendsofphp/php-cs-fixer": "^2.16",
    "overtrue/phplint": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "BlueprintDraftFromMySQLSource\\": "src"
